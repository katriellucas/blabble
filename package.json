    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^8.56.10",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.41.0",
    "globals": "^15.8.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "prisma": "^5.16.2",
    "svelte": "^5.0.0-next.181",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.3.3",
    "vitest": "^2.0.2"
